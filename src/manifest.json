{
  "name": "Lift template server",
  "version": "1",
  "manifest_version": 2,
  "description": "An embedded HTTP server designed for helping designers work with Lift templates",
  "icons": {
    "16": "icon-16.png",
    "128": "icon-128.png"
  },
  "default_locale": "en",
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "permissions": ["experimental", {"socket":["tcp-connect", "tcp-listen"]}]
}
